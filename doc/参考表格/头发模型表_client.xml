<config>
	<!-- 
字段顺序必与结构中定义的一样
name 字段名字
type 字段类型：（缺省 int ）
	string	字符串(size 缺省 16); 
	int		整数（根据size决定其位数,缺省 4，base：进制，缺省 10）;
	float	浮点数（size = 4 单精度，size = 8 双精度,缺省 4）
default 缺省值
-->
	<table name="HairDataBase" ExcelFile="头发模型表.xlsm" sheet="Sheet1" sort="编号+性别">
		<fields>
			<field name="编号" type="int" size="2" var="wdID"/>
			<field name="性别" type="int" size="2" var="wdSex"/>
			<field name="名称" type="string" size="0" var="strName"/>
			<field name="图标" type="int" size="4" var="dwIcon"/>
			<field name="图标组" type="int" size="4" var="dwIconGrp"/>
			<field name="模型名" type="string" size="0" var="strModel"/>
			<field name="说明" type="string" size="0" var="strDesc"/>
		</fields>
	</table>
</config>	


