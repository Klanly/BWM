<config>
	<!-- 
字段顺序必与结构中定义的一样
name 字段名字
type 字段类型：（缺省 int ）
	string	字符串( size 缺省 16); 
	int		整数（根据size决定其位数,缺省 4，base：进制，缺省 10）;
	float	浮点数（size = 4 单精度，size = 8 双精度,缺省 4）
default 缺省值
-->
	<table name="FashionDataBase" ExcelFile="时装表.xlsm" sheet="Sheet1" sort="编号">
		<fields>
			<field name="编号" type="int" size="4" var="dwID"/>
			<field name="名称" type="string" size="0" var="strName"/>
			<field name="类型" type="int" size="2" var="wType"/>
			<field name="价格" type="int" size="4" var="dwPrice"/>
			<field name="使用要求" type="int" size="2" var="wCondition"/>
			<field name="生命" type="int" size="4" var="dwHP"/>
			<field name="物攻" type="int" size="4" var="dwPDamage"/>
			<field name="法攻" type="int" size="4" var="dwMDamage"/>
			<field name="物防" type="int" size="4" var="dwPDefence"/>
			<field name="法防" type="int" size="4" var="dwMDefence"/>
		</fields>
	</table>
</config>
