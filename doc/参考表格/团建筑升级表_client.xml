	<table name="BuildingLevelBase" ExcelFile="团建筑升级表.xlsm" sheet="Sheet1" sort="类型+等级">
		<fields>
			<field name="类型" type="int" size="2" var="wdID"/>
			<field name="等级" type="int" size="2" var="wdLevel"/>
