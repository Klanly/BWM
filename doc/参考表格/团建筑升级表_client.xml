			<field name="升级团资金" type="int" size="4" var="dwLevelUpZijin"/>
			<field name="人数上限" type="int" size="4" var="dwNumLimit"/>
			<field name="名称" type="string" size="0" var="strName"/>
			<field name="总繁荣度" type="int" size="4" var="dwTotalFanrong"/>
			<field name="下一级可购买物品" type="string" size="0" var="strNextLevelGoods"/>
		</fields>
	</table>
