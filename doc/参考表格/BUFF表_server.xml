<config>
	<!-- 
字段顺序必与结构中定义的一样
name 字段名字
type 字段类型：（缺省 int ）
	string	字符串(size 缺省 16); 
	int		整数（根据size决定其位数,缺省 4，base：进制，缺省 10）;
	float	浮点数（size = 4 单精度，size = 8 双精度,缺省 4）
default 缺省值
-->
	<table name="BuffDataBase"  sheet="Sheet1" ExcelFile="BUFF表.xlsm">
		<fields>
			<field name="编号" type="int"/>
			<field name="名称" type="string" size="32"/>
			<field name="主动取消" type="int"/>
			<field name="BUFF类型" type="int"/>
			<field name="显示位置" type="int"/>
			<field name="是否存档" type="int"/>
			<field name="发送九屏" type="int"/>
			<field name="死亡不清除" type="int"/>
			<field name="无限时间" type="int"/>
			<field name="是否计算战斗力" type="int"/>
		</fields>
	</table>
</config>
