<config>
	<!-- 
字段顺序必与结构中定义的一样
name 字段名字
type 字段类型：（缺省 int ）
	string	字符串( size 缺省 16); 
	int		整数（根据size决定其位数,缺省 4，base：进制，缺省 10）;
	float	浮点数（size = 4 单精度，size = 8 双精度,缺省 4）
default 缺省值
-->
	<table name="NewHorseLevelupDatabase" ExcelFile="坐骑进阶表.xlsm" sheet="Sheet1" sort="坐骑id">
		<fields>
			<field name="坐骑id" type="int" size="4" var="dwID"/>
			<field name="等阶" type="int" size="4" var="dwLevel"/>
			<field name="进化id" type="int" size="4" var="dwLevelupID"/>
			<field name="满祝福" type="int" size="4" var="dwMaxLuckyValue"/>
			<field name="道具消耗" type="int" size="4" var="dwMaterialNum"/>
			<field name="绑银消耗" type="int" size="4" var="dwMoney"/>
			<field name="速度" type="int" size="4" var="dwSpeed"/>
			<field name="血" type="int" size="4" var="dwHp"/>
			<field name="防" type="int" size="4" var="dwDef"/>
			<field name="攻" type="int" size="4" var="dwAttack"/>
			<field name="伤害反射2" type="int" size="4" var="dwHurtecho2"/>
			<field name="闪避2" type="int" size="4" var="dwHide2"/>
			<field name="减伤2" type="int" size="4" var="dwSub2"/>
			<field name="致命一击2" type="int" size="4" var="dwLucky2"/>
			<field name="进化小时" type="int" size="4" var="dwLevelupTime"/>
		</fields>
	</table>
</config>
