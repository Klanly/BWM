	<table name="NewGuideContentBase" sheet="Sheet1" ExcelFile="引导内容表格.xlsm" sort="编号">
		<fields>
			<field name="编号" type="int" size="4" var="dwID"/>
			<field name="引导内容" type="string" size="0" var="strContent"/>
			<field name="等级" type="int" size="4" var="dwLevel"/>
			<field name="hotkey" type="int" size="4" var="dwHotkeyID"/>
			<field name="任务ID" type="string" size="0" var="strTaskIDs"/>
			<field name="说明" type="string" size="0" var="strTips"/>
			<field name="图标" type="int" size="4" var="dwIcon"/>
		</fields>
	</table>
</config>
