<config>
	<!-- 
字段顺序必与结构中定义的一样
name 字段名字
type 字段类型：（缺省 int ）
	string	字符串( size 缺省 16); 
	int		整数（根据size决定其位数,缺省 4，base：进制，缺省 10）;
	float	浮点数（size = 4 单精度，size = 8 双精度,缺省 4）
default 缺省值
-->
	<table name="BossCard" ExcelFile="boss卡片表.xlsm" sheet="Sheet1" sort="卡片编号">
		<fields>
			<field name="卡片编号" type="int" size="4" var="dwID"/>
			<field name="前缀" type="int" size="4" var="dwPro"/>
			<field name="npcid" type="int" size="4" var="dwnpcid"/>
			<field name="卡片名称" type="string" size="0" var="dwCardname"/>
			<field name="卡片图标" type="int" size="4" var="dwCardpic"/>			
		</fields>
	</table>
</config>
