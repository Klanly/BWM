<config>
	<!-- 
字段顺序必与结构中定义的一样
name 字段名字
type 字段类型：（缺省 int ）
	string	字符串( size 缺省 16); 
	int		整数（根据size决定其位数,缺省 4，base：进制，缺省 10）;
	float	浮点数（size = 4 单精度，size = 8 双精度,缺省 4）
default 缺省值
-->
	<table name="IconCompareBase" ExcelFile="图标对照表.xlsm" sheet="Sheet1" sort="ID+类型" distinct="false">
		<fields>
			<field name="ID" type="int" size="4" var="dwID"/>
			<field name="类型" type="int" size="4" var="dwType"/>
			<field name="名称" type="string" size="0" var="strname"/>
			<field name="图标" type="int" size="4" var="dwIcon"/>
			<field name="大图标组" type="int" size="4" var="dwBigIconGrp"/>
			<field name="小图标组" type="int" size="4" var="dwSmallIconGrp"/>
			<field name="粒子特效" type="string" size="0" var="strParticle"/>
		</fields>
	</table>
</config>
