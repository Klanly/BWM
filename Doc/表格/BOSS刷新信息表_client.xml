<config>
	<!-- 
字段顺序必与结构中定义的一样
name 字段名字
type 字段类型：（缺省 int ）
	string	字符串( size 缺省 16); 
	int		整数（根据size决定其位数,缺省 4，base：进制，缺省 10）;
	float	浮点数（size = 4 单精度，size = 8 双精度,缺省 4）
default 缺省值
-->
	<table name="BossInfoDataBase" ExcelFile="BOSS刷新信息表.xlsm" sheet="Sheet1" sort="类型+BOSS编号">
		<fields>
			<field name="类型" type="int" size="4" var="dwID"/>
			<field name="BOSS编号" type="int" size="4" var="dwNpcID"/>
			<field name="刷新时间" type="string" size="0" var="strTime"/>
			<field name="地图ID" type="int" size="4" var="dwMapID"/>
			<field name="描述" type="string" size="0" var="strDesc"/>
			<field name="地点" type="string" size="0" var="strLocation"/>
			<field name="BOSS坐标X" type="int" size="4" var="dwNpcPosX"/>		
			<field name="BOSS坐标Y" type="int" size="4" var="dwNpcPosY"/>		
			<field name="人类进程" type="int" size="4" var="dwWorldLevel"/>		
			<field name="BOSS名字图片" type="int" size="4" var="dwNamePic"/>					
		</fields>
	</table>
</config>
