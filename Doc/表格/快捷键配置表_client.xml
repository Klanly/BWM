<config>
	<!-- 
字段顺序必与结构中定义的一样
name 字段名字
type 字段类型：（缺省 int ）
	string	字符串(size 缺省 16); 
	int		整数（根据size决定其位数,缺省 4，base：进制，缺省 10）;
	float	浮点数（size = 4 单精度，size = 8 双精度,缺省 4）
default 缺省值
-->
	<table name="HotKeyDataBase" ExcelFile="快捷键配置表.xlsm" sheet="Sheet1" sort="编号">
		<fields>
			<field name="编号" type="int" size="4" var="dwID"/>
			<field name="玩家可见" type="int" size="4" var="dwVisible"/>
			<field name="分类" type="int" size="4" var="dwType"/>
			<field name="名称" type="string" size="0" var="strName"/>
			<field name="默认快捷键0" type="string" size="0" var="strDefaultKey0"/>
			<field name="默认快捷键1" type="string" size="0" var="strDefaultKey1"/>
			<field name="默认快捷键2" type="string" size="0" var="strDefaultKey2"/>
			<field name="默认快捷键3" type="string" size="0" var="strDefaultKey3"/>
			<field name="关联打开" type="int" size="4" var="dwRelationID"/>
			<field name="引导ID" type="int" size="4" var="dwIntroID"/>
			<field name="引导等级" type="int" size="4" var="dwNeedLevel"/>
			<field name="悬浮提示" type="string" size="0" var="strTips"/>
		</fields>
	</table>
</config>	

