<config>
	<!-- 
字段顺序必与结构中定义的一样
name 字段名字
type 字段类型：（缺省 int ）
	string	字符串(size 缺省 16); 
	int		整数（根据size决定其位数,缺省 4，base：进制，缺省 10）;
	float	浮点数（size = 4 单精度，size = 8 双精度,缺省 4）
default 缺省值
-->
	<table name="SacredStoneDataBase" ExcelFile="圣石表.xlsm" sheet="Sheet1" sort="圣石ID+品质+等级">
		<fields>
			<field name="圣石ID" type="int" size="4" var="dwID"/>
			<field name="品质" type="int" size="2" var="wdQuality"/>
			<field name="等级" type="int" size="2" var="wdLevel"/>
			<field name="物攻" type="int" size="4" var="dwPdam"/>
			<field name="魔攻" type="int" size="4" var="dwMdam"/>
			<field name="物防" type="int" size="4" var="dwPdef"/>
			<field name="魔防" type="int" size="4" var="dwMdef"/>
			<field name="生命" type="int" size="4" var="dwMaxhp"/>
			<field name="携带魔阵经验和" type="int" size="4" var="dwTotalExp"/>
		</fields>
	</table>
</config>
