				<field name="总资质上区间" type="int" size="4" var="dwTotalMaxValue" />
				<field name="单项最小值" type="int" size="4" var="dwMinValue"/>
				<field name="单项最大值" type="int" size="4" var="dwMaxValue"/>
				<field name="骑魂合成费用" type="int" size="4" var="dwHeChengCost" />
				<field name="骑魂合成胜率" type="int" size="4" var="dwPercent" />
				<field name="坐骑融合费用" type="int" size="4" var="dwRongHeCost" />
