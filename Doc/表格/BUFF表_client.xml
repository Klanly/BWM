<config>
	<!-- 
字段顺序必与结构中定义的一样
name 字段名字
type 字段类型：（缺省 int ）
	string	字符串(size 缺省 16); 
	int		整数（根据size决定其位数,缺省 4，base：进制，缺省 10）;
	float	浮点数（size = 4 单精度，size = 8 双精度,缺省 4）
default 缺省值
-->
	<table name="BuffDataBase" ExcelFile="BUFF表.xlsm" sheet="Sheet1" sort="编号">
		<fields>
			<field name="编号" type="int" size="4" var="dwID"/>
			<field name="名称" type="string" size="0" var="strName"/>
			<field name="BUFF时间显示" type="int" size="4" var="dwShowTime"/>
			<field name="显示屏蔽" type="int" size="4" var="dwShield"/>
			<field name="显示位置" type="int" size="4" var="dwShowPos"/>
			<field name="主动取消" type="int" size="4" var="dwCanel"/>
			<field name="是否多线程加载" type="int" size="4" var="dwUseMultiThread"/>
			<field name="描述" type="string" size="0" var="strDesc"/>
			<field name="2D特效" type="int" size="4" var="dwEffect2D"/>
			</fields>
	</table>
</config>	


